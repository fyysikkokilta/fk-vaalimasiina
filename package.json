{
  "name": "fk-vaalimasiina",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build --turbo",
    "start": "next start",
    "lint": "next lint",
    "ci": "pnpm db:migrate && pnpm build",
    "generate-election": "tsx ./tests/fixtures/generate-election.ts",
    "test:unit": "vitest",
    "type:check": "tsc --noEmit",
    "prettier:format": "prettier --write .",
    "db:generate-migration": "drizzle-kit generate",
    "db:migrate": "tsx ./src/db/migrate.ts",
    "preinstall": "pnpx only-allow pnpm",
    "prepare": "simple-git-hooks"
  },
  "lint-staged": {
    "**/*.ts?(x)": "bash -c tsc -p tsconfig.json --noEmit",
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write --ignore-unknown"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpx lint-staged"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@next/bundle-analyzer": "^15.3.3",
    "@react-email/components": "^0.0.41",
    "@tailwindcss/postcss": "^4.1.8",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^12.0.2",
    "next": "^15.3.3",
    "next-intl": "^4.1.0",
    "next-safe-action": "^8.0.2",
    "postcss": "^8.5.4",
    "postgres": "^3.4.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-toastify": "^11.0.5",
    "seedrandom": "^3.0.5",
    "tailwindcss": "^4.1.8",
    "tsx": "^4.19.4",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@playwright/test": "^1.52.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/seedrandom": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.5.1",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "simple-git-hooks": "^2.13.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "simple-git-hooks",
      "unrs-resolver"
    ]
  }
}
